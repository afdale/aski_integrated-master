<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:background="@drawable/artboard_1"
        android:layout_height="match_parent"
        tools:context=".OnProgressActivity">

    <TextView
            android:text=":"
            android:textColor="@color/black"
            android:fontFamily="@font/bahnschrift"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv7"
            android:textSize="16sp" android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/technician"
            app:layout_constraintTop_toTopOf="@+id/technician"/>
    <TextView
            android:text="No. Mold"
            android:fontFamily="@font/bahnschrift"
            android:textColor="@color/black"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:id="@+id/judulProblem" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            android:layout_marginTop="16dp" android:textSize="18sp"
            android:textStyle="bold" app:layout_constraintTop_toBottomOf="@+id/waktu12"/>
    <TextView
            android:text="Waktu Repair :"
            android:fontFamily="@font/bahnschrift"
            android:textColor="@color/black"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/waktu12"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="8dp" android:textSize="18sp"
            android:textStyle="bold"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/waktuRepairCMeter"/>
    <TextView
            android:text="N/A"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/nomcdpTV"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintTop_toTopOf="@+id/judulProblem"
            app:layout_constraintStart_toEndOf="@+id/tv4"
            android:layout_marginStart="8dp"/>

    <Chronometer
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/bahnschrift"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:id="@+id/waktuRepairCMeter"
            android:textSize="18sp"/>
    <TextView
            android:text="Problem"
            android:fontFamily="@font/bahnschrift"
            android:textColor="@color/black"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:id="@+id/tv123555" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:textSize="18sp" android:textStyle="bold"
            android:layout_marginTop="24dp" app:layout_constraintTop_toBottomOf="@+id/techd"/>
    <TextView
            android:text="Technician1"
            android:textColor="@color/black"
            android:fontFamily="@font/bahnschrift"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:id="@+id/technician"
            android:textSize="16sp" android:textStyle="bold"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/nomcdpTV" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"/>
    <TextView
            android:textColor="@color/black"
            android:text="Technician3"
            android:fontFamily="@font/bahnschrift"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
