            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/techb"/>
    <TextView
            android:textColor="@color/black"
            android:text="Technician2"
            android:fontFamily="@font/bahnschrift"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:id="@+id/technician2"
            android:textSize="16sp" android:textStyle="bold"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/techa" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:text="N/A"
            android:fontFamily="@font/bahnschrift"
            android:textStyle="bold"
            android:layout_width="241dp"
            android:layout_height="wrap_content"
            android:id="@+id/techa"
            android:textSize="18sp"
            app:layout_constraintStart_toEndOf="@+id/technician"
            android:layout_marginStart="12dp"
            app:layout_constraintTop_toTopOf="@+id/technician"/>
    <TextView
            android:text="N/A"
            android:fontFamily="@font/bahnschrift"
            android:textStyle="bold"
            android:layout_width="241dp"
            android:layout_height="wrap_content"
            android:id="@+id/techd"
            android:textSize="18sp"
            app:layout_constraintTop_toTopOf="@+id/technician4" app:layout_constraintStart_toEndOf="@+id/technician4"
            android:layout_marginStart="12dp"/>
    <TextView
            android:text="N/A"
            android:fontFamily="@font/bahnschrift"
            android:textStyle="bold"
            android:layout_width="241dp"
            android:layout_height="wrap_content"
            android:id="@+id/techc"
            android:textSize="18sp"
            app:layout_constraintStart_toEndOf="@+id/tv8"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="@+id/technician3"/>
    <TextView
            android:text="N/A"
            android:fontFamily="@font/bahnschrift"
            android:textStyle="bold"
            android:layout_width="241dp"
            android:layout_height="wrap_content"
            android:id="@+id/techb"
            android:textSize="18sp"
            app:layout_constraintTop_toTopOf="@+id/technician2" app:layout_constraintStart_toEndOf="@+id/technician2"
            android:layout_marginStart="12dp"/>
    <TextView
            android:text="Perbaikan"
            android:textColor="@color/black"
            android:fontFamily="@font/bahnschrift"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:id="@+id/perbaikan" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:textSize="18sp" android:textStyle="bold" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/problemET"/>
    <TextView
            android:text="Jenis Problem"
            android:textColor="@color/black"
            android:fontFamily="@font/bahnschrift"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:id="@+id/tv3"
            android:textSize="18sp" android:textStyle="bold"
            android:textAlignment="center"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/perbaikan"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.9"/>
    <TextView
            android:text=":"
            android:textColor="@color/black"
            android:fontFamily="@font/bahnschrift"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv123412"
            android:textSize="18sp" android:textStyle="bold" app:layout_constraintStart_toEndOf="@+id/tv123555"
            app:layout_constraintTop_toTopOf="@+id/tv123555"/>
    <TextView
            android:text=":"
            android:textColor="@color/black"
            android:fontFamily="@font/bahnschrift"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv6"
            android:textSize="16sp" android:textStyle="bold"
            app:layout_constraintTop_toTopOf="@+id/technician4" app:layout_constraintStart_toEndOf="@+id/technician4"/>
    <TextView
            android:text=":"
            android:textColor="@color/black"
            android:fontFamily="@font/bahnschrift"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv8"
            android:textSize="16sp" android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/technician3"
            app:layout_constraintTop_toTopOf="@+id/technician3"/>
    <TextView
            android:text=":"
            android:textColor="@color/black"
            android:fontFamily="@font/bahnschrift"
            android:layout_width="4dp"
            android:layout_height="wrap_content"
            android:id="@+id/tv5"
            android:textSize="16sp" android:textStyle="bold"
            app:layout_constraintTop_toTopOf="@+id/technician2" app:layout_constraintStart_toEndOf="@+id/technician2"
    />
    <TextView
            android:text=":"
            android:textColor="@color/black"
            android:fontFamily="@font/bahnschrift"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
