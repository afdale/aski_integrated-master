            android:text=":"
            android:textColor="@color/black"
            android:fontFamily="@font/bahnschrift"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv2"
            android:textSize="18sp" android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/perbaikan"
            app:layout_constraintTop_toTopOf="@+id/perbaikan"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/bahnschrift"
            android:textStyle="bold"
            android:ems="10"
            android:id="@+id/problemET"
            app:layout_constraintStart_toEndOf="@+id/tv123412"
            android:layout_marginStart="8dp" android:layout_marginEnd="5dp" app:layout_constraintEnd_toEndOf="parent"
            android:inputType="textMultiLine" android:hint="Problem"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintTop_toTopOf="@+id/tv123555"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:id="@+id/PerbaikanET"
            android:fontFamily="@font/bahnschrift"
            android:textStyle="bold"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:inputType="textMultiLine"
            android:hint="Detail Perbaikan"
            app:layout_constraintStart_toEndOf="@+id/tv2"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="@+id/perbaikan"/>
    <EditText
            android:layout_width="100dp"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:fontFamily="@font/bahnschrift"
            android:layout_height="wrap_content"
            android:ems="10"
            android:id="@+id/jenisProblemET"
            app:layout_constraintEnd_toEndOf="parent"
            android:inputType="textMultiLine"
            android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/tv3" android:hint="Jenis" android:textAlignment="center"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.9"
    />
    <ImageButton
            android:layout_width="150dp"
            android:scaleType="fitCenter"
            android:background="#00040303"
            android:layout_height="50dp" app:srcCompat="@drawable/finish"
            android:id="@+id/finishProgressBTN"
            app:layout_constraintTop_toTopOf="@+id/cancelProgressBTN"
            app:layout_constraintEnd_toStartOf="@+id/cancelProgressBTN"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.0"/>
    <ImageButton
            android:layout_width="150dp"
            android:scaleType="fitCenter"
            android:background="#00040303"
            android:layout_height="50dp" app:srcCompat="@drawable/cancel"
            android:id="@+id/cancelProgressBTN"
            android:onClick="goCancelProgress" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/jenisProblemET"
            app:layout_constraintStart_toEndOf="@+id/finishProgressBTN"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.0"
            android:layout_marginTop="24dp"/>
    <TextView
            android:text="Technician4"
            android:fontFamily="@font/bahnschrift"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:id="@+id/technician4"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/techc"/>


</androidx.constraintlayout.widget.ConstraintLayout>